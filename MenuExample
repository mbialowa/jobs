<script type="text/javascript">
var rememberLastLink=10;

$(document).ready(function(){
	
$("#menu li div table tr td div[class*='columnSize']").mouseenter(function(){
	$("#"+rememberLastLink+" a").css({"background-color":"transparent", "color":"#000000"});
	var idBox=$(this).attr('id');
	var infoBox = String(idBox).charAt(1);
	$("#infoBox"+infoBox).html($("#div"+idBox).html());
  $("#"+idBox+" a").css({"background-color":"#336633", "color":"#ffffff"});
	rememberLastLink=idBox;
});
	
$("#menu li div table tr td div[class*='columnSize']").mouseleave(function(){
  $("#"+rememberLastLink+" a").css({"background-color":"#336633", "color":"#ffffff"});
});

$("#menu li").mouseenter(function(){
  $("#"+rememberLastLink+" a").css({"background-color":"transparent", "color":"#000000"});
	var idBox=$(this).attr('id');
	$("#infoBox"+idBox).html($("#div1"+idBox).html());
  $("#1"+idBox+" a").css({"background-color":"#336633", "color":"#ffffff"});
});

});
</script>
